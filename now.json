{
  "version": 2,
  "name": "repo-remover",
  "alias": [
    "reporemover.xyz"
  ],
  "builds": [
    {
      "src": "package.json",
      "use": "@now/static-build"
    }
  ],
  "routes": [
    {
      "src": "/js/(.*)",
      "dest": "js/$1"
    },
    {
      "src": "/css/(.*)",
      "dest": "css/$1"
    },
    {
      "src": "/[^\\.]+?(\\.html)?$",
      "dest": "index.html"
    }
  ]
}
